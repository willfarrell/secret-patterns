{
  "Slack Token": "(xox[p|b|o|a]-[0-9]{12}-[0-9]{12}-[0-9]{12}-[a-z0-9]{32})",
  "Facebook Oauth": "[f|F][a|A][c|C][e|E][b|B][o|O][o|O][k|K].*${quote}[0-9a-f]{32}${quote}",
  "Twitter Oauth": "[t|T][w|W][i|I][t|T][t|T][e|E][r|R].*${quote}[0-9a-zA-Z]{35,44}${quote}",
  "GitHub": "[g|G][i|I][t|T][h|H][u|U][b|B].*${quote}[0-9a-zA-Z]{35,40}${quote}",
  "Google Oauth": "(${quote}client_secret${quote}${connect}${quote}[a-zA-Z0-9-_]{24}${quote})",
  "Heroku API Key": "[h|H][e|E][r|R][o|O][k|K][u|U].*[0-9A-F]{8}-[0-9A-F]{4}-[0-9A-F]{4}-[0-9A-F]{4}-[0-9A-F]{12}",
  "Slack Webhook": "https://hooks.slack.com/services/T[a-zA-Z0-9_]{8}/B[a-zA-Z0-9_]{8}/[a-zA-Z0-9_]{24}",
  "Google (GCP) Service-account": "${quote}type${quote}${connect}${quote}service_account${quote},?",
  "Twilio API Key": "SK[a-z0-9]{32}",
  "AWS Access Key ID Value": "(A3T[A-Z0-9]|AKIA|AGPA|AIDA|AROA|AIPA|ANPA|ANVA|ASIA)[A-Z0-9]{16}",
  "AWS Account ID": "${opt_quote}(AWS|aws|Aws)?_?(ACCOUNT|account|Account)_?(ID|id|Id)?${opt_quote}${connect}${opt_quote}[0-9]{4}-?[0-9]{4}-?[0-9]{4}${opt_quote}",
  "AWS Access Key ID": "${opt_quote}(AWS|aws|Aws)?_?(ACCESS|access|Access)_?(KEY|key|Key)?_?(ID|id|Id)?${opt_quote}${connect}${opt_quote}(A3T[A-Z0-9]|AKIA|AGPA|AIDA|AROA|AIPA|ANPA|ANVA|ASIA)[A-Z0-9]{16}${opt_quote}",
  "AWS Secret Access Key": "${opt_quote}(AWS|aws|Aws)?_?(SECRET|secret|Secret)_?(ACCESS|access|Access)?_?(KEY|key|Key)?_?(ID|id|Id)?${opt_quote}${connect}${opt_quote}[A-Za-z0-9/+=]{40}${opt_quote}",
  "AWS Session Token": "${opt_quote}(AWS|aws|Aws)?_?(SESSION|session|Session)?_?(TOKEN|token|Token)?${opt_quote}${connect}${opt_quote}[A-Za-z0-9/+=]{16,4096}${opt_quote}",

  "RSA private key": "-----BEGIN RSA PRIVATE KEY-----",
  "SSH (OPENSSH) private key": "-----BEGIN OPENSSH PRIVATE KEY-----",
  "SSH (DSA) private key": "-----BEGIN DSA PRIVATE KEY-----",
  "SSH (EC) private key": "-----BEGIN EC PRIVATE KEY-----",
  "PGP private key block": "-----BEGIN PGP PRIVATE KEY BLOCK-----",
  "URL Authorization": "[a-zA-Z]{3,10}://[^/${space}:@]{3,20}:[^/${space}:@]{3,20}@.{1,100}[\"'${space}]",
  "Generic Secret": "[s|S][e|E][c|C][r|R][e|E][t|T].*${quote}[0-9a-zA-Z]{32,64}${quote}",
  "Generic API Key": "[a|A][p|P][i|I][_]?[k|K][e|E][y|Y].*${quote}[0-9a-zA-Z]{32,64}${quote}",
  "Generic Password": "[p|P][a|A][s|S][s|S][w|W]?[o|O][r|R]?[d|D]?.*${quote}.{8,64}${quote}",
  "Generic Token": "[t|T][o|O][k|K][e|E][n|N].*${quote}.{8,64}${quote}"
}
